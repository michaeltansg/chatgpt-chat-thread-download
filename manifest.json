{
  "manifest_version": 3,
  "name": "ChatGPT Chat Thread Download",
  "version": "0.1.0",
  "icons" : {
    "16" : "icon.png",
    "48" : "icon.png",
    "128" : "icon.png"
  },
  "permissions": [
    "scripting",
    "tabs"
  ],
  "action" : {
    "default_icon" : "icon.png",
    "default_title" : "Download Chat Thread"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions" : [
    "https://chat.openai.com/*"
  ]
}